type Result = 
 variant {
   err: text;
   ok;
 };
type EmailPermission = 
 variant {
   Allow: text;
   Deny;
 };
service : {
  accept_risk_warning: () -> (Result);
  get_email_permission: () -> (opt EmailPermission) query;
  get_risk_warning_agreement: () -> (opt bool) query;
  insert_email: (email: opt text) -> (Result);
}
